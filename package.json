{
    "name": "umbra-syntax-highlight",
    "description": "Syntax highlighting for Umbra Language",
    "displayName": "Umbra Syntax Highlight",
    "publisher": "pmqueiroz",
    "readme": "README.md",
    "version": "0.10.0",
    "license": "MIT",
    "author": {
        "name": "Pedro Queiroz",
        "url": "https://pmqueiroz.com"
    },
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/umbra-lang/vscode-umbra"
    },
    "engines": {
        "vscode": "^1.63.0"
    },
    "keywords": [
        "umbra",
        "vscode",
        "language",
        "highlight",
        "syntax"
    ],
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "Umbra",
            "aliases": ["Umbra Language", "Umbra"],
            "extensions": [".umb", ".u"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "Umbra",
            "scopeName": "source.umbra",
            "path": "./syntaxes/Umbra.tmLanguage.json"
        }]
    }
}
